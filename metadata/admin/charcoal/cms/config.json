{
    "admin": {
        "lists": {
            "cms.default": {
                "label": {
                    "en": "Configs",
                    "fr": "Configs"
                },
                "properties": [
                    ""
                ],
                "listActions": [
                    {
                        "label": {
                            "en": "New config",
                            "fr": "Nouvel config"
                        },
                        "ident": "create",
                        "url": "object/edit?obj_type={{objType}}"
                    }
                ],
                "orders": {
                    "position": {
                        "property": "position",
                        "mode": "asc"
                    }
                }
            }
        },

        "forms": {
            "cms.default": {
                "label": "Configuration",
                "groups": {
                    "cms.general": {
                        "priority": 100
                    },
                    "cms.sharer": {
                        "priority": 300
                    },
                    "cms.meta": {
                        "priority": 400
                    },
                    "cms.revisions": {
                        "priority": 8000
                    }
                },
                "sidebars": {
                    "action": {
                        "permissions": {
                            "footer": [
                                "dev-footer"
                            ]
                        },
                        "title": "Actions",
                        "properties": [],
                        "actions": {}
                    }
                }
            }
        },

        "formGroups": {
            "cms.general": {
                "layout": {
                    "structure": [
                        {"columns": [1]},
                        {"columns": [1]},
                        {"columns": [1, 1]}
                    ]
                },
                "title": {
                    "en": "General",
                    "fr": "Général"
                },
                "showTitle": false,
                "icon": "wrench",
                "properties": [
                    "defaultFromEmail"
                ]
            },
            "cms.sharer": {
                "layout": {
                    "structure": [
                        {"columns": [1, 1]},
                        {"columns": [1, 1]},
                        {"columns": [1, 1]},
                        {"columns": [1, 1]},
                        {"columns": [1, 1]}
                    ]
                },
                "title": {
                    "en": "Social media",
                    "fr": "Média sociaux"
                },
                "showTitle": false,
                "icon": "share-square",
                "template": "charcoal/admin/widget/form-group/structure",
                "type": "charcoal/admin/widget/form-group/structure",
                "storageProperty": "social_medias"
            },
            "cms.meta": {
                "layout": {
                    "structure": [
                        {"columns": [1, 1]},
                        {"columns": [1, 1]}
                    ]
                },
                "title": {
                    "en": "Meta tags",
                    "fr": "Tags Méta"
                },
                "showTitle": false,
                "icon": "tag",
                "properties": [
                    "defaultMetaTitle",
                    "defaultMetaDescription",
                    "defaultMetaUrl",
                    "defaultMetaImage"
                ]
            },
            "cms.revisions": {
                "title": {
                    "fr": "Révisions",
                    "en": "Revisions"
                },
                "showTitle": false,
                "showHeader": false,
                "icon": "floppy-o",
                "template": "charcoal/admin/widget/form-group/object-revisions",
                "type": "charcoal/admin/widget/form-group/object-revisions"
            }
        },

        "dashboards": {
            "admin.edit": {
                "layout": {
                    "structure": [
                        {"columns": [1]}
                    ]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "formIdent": "cms.default",
                        "request": "main_menu=configuration",
                        "groupDisplayMode": "tab"
                    }
                }
            },
            "admin.table": {
                "layout": {
                    "structure": [
                        {"columns": [1]}
                    ]
                },
                "widgets": {
                    "table": {
                        "type": "charcoal/admin/widget/table",
                        "collectionIdent": "cms.default",
                        "sortable": true
                    }
                }
            }
        },

        "defaultCreateDashboard": "admin.edit",
        "defaultEditDashboard": "admin.edit",
        "defaultCollectionDashboard": "admin.table"
    }
}
