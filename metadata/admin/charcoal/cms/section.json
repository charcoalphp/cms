{
    "admin": {
        "lists": {
            "section": {
                "properties": [
                    "id",
                    "title",
                    "active"
                ]
            }
        },

        "defaultList": "section",

        "forms": {
            "default": {
                "groups": {
                    "infos": {
                        "priority": 1,
                        "title": {
                            "en": "Informations",
                            "fr": "Informations"
                        },
                        "properties": [
                            "id",
                            "sectionType",
                            "templateIdent",
                            "templateOptions",
                            "active"
                        ]
                    },
                    "content": {
                        "priority": 11,
                        "title": {
                            "en": "Content",
                            "fr": "Contenu"
                        },
                        "properties": [
                            "title",
                            "subtitle",
                            "content",
                            "image"
                        ]
                    },
                    "metadata": {
                        "priority": 31,
                        "title": {
                            "en": "Metadata",
                            "fr": "Metadata"
                        },
                        "properties": [
                            "metaTitle",
                            "metaDescription",
                            "metaImage"
                        ]
                    }
                },
                "sidebars": {
                    "default": {}
                }
            }
        },

        "defaultForm": "default",

        "dashboards": {
            "admin.edit": {
                "layout": {
                    "structure": [
                        {
                            "columns": [1]
                        }
                    ]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "formIdent": "default",
                        "objType": "charcoal/cms/section"
                    }
                }
            },
            "admin.table": {
                "layout": {
                    "structure": [
                        {
                            "columns": [1]
                        }
                    ]
                },
                "widgets": {
                    "list": {
                        "type": "charcoal/admin/widget/table",
                        "label": "Table label",
                        "objType": "charcoal/cms/section"
                    }
                }
            }
        },

        "defaultEditDashboard": "admin.edit",
        "defaultCollectionDashboard": "admin.table"
    }
}
